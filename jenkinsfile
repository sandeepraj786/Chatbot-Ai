pipeline {
    agent any
    stages {
	    stage('SCM Checkout'){
             steps{    
		    git 'https://github.com/sandeepraj786/Chatbot-Ai.git'
		 }
	    }
            stage('Run Ansible playbook') {
               steps {
                     sh '/var/lib/jenkins/workspace/Chatbot-Ai/ansible ansible-playbook ecs_ansible.yml'
	             
         }
       }
    }
}
	    
